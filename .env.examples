# Google Cloud Configuration
# Copy this file to .env and fill in your actual values

# Path to your Google Cloud service account credentials JSON file
# Required for authenticating with Google Cloud services
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/credentials.json

# Your Google Cloud Project ID for Vertex AI
# The project where your Vertex AI models are deployed
GOOGLE_VERTEX_PROJECT=your-project-id

# Google Cloud region/location for Vertex AI
# Common locations: us-central1, us-east1, asia-south1, etc.
GOOGLE_VERTEX_LOCATION=us-central1

# Your Google Cloud Project ID (can be same as GOOGLE_VERTEX_PROJECT)
# Used for general Google Cloud API operations
GOOGLE_CLOUD_PROJECT_ID=your-project-id
